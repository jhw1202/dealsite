@font-face {
    font-family: 'latoregular';
    src: url('<%= font_path('lato-regular/lato-regular-webfont.eot') %>');
    src: url('<%= font_path('lato-regular/lato-regular-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= font_path('lato-regular/lato-regular-webfont.woff') %>') format('woff'),
         url('<%= font_path('lato-regular/lato-regular-webfont.ttf') %>') format('truetype'),
         url('<%= font_path('lato-regular/lato-regular-webfont.svg#latoregular') %>') format('svg');
    font-weight: normal;
    font-style: normal;
}


